{
  "name": "graphql-fetch",
  "version": "0.0.0",
  "description": "A lightweight isomorphic GraphQL client.",
  "license": "MIT",
  "main": "dist/index.js",
  "author": "Jordan Phillips",
  "repository": "https://github.com/PHILLIPS71/graphql-fetch",
  "bugs": "https://github.com/PHILLIPS71/graphql-fetch/issues",
  "scripts": {
    "lint": "eslint --fix --ext .ts ./src",
    "format": "pretty-quick --staged",
    "check-types": "tsc --pretty",
    "validate": "npm-run-all --parallel check-types format lint",
    "test": "jest",
    "build:src": "tsc --project tsconfig.json",
    "build:paths": "resolve-tspaths",
    "build": "npm-run-all build:*"
  },
  "dependencies": {
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "isomorphic-fetch": "^3.0.0",
    "web-streams-polyfill": "^3.2.1"
  },
  "peerDependencies": {
    "graphql": "14 - 16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "graphql": "14",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "resolve-tspaths": "^0.6.0",
    "typescript": "^4.7.4"
  }
}
